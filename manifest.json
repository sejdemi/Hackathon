
{
    "manifest_version": 2,
    "name": "chromeEYE",
    "description": "null",
    "version": "1.0",
    "author": "BB/Sejan",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["style.css"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
      "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "eye.png"

        }
   }
}
   
   

